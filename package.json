{
  "name": "interface-nodejs",
  "version": "1.0.0",
  "description": "Interface moderna em Node.js",
  "main": "electron.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "electron": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-simple": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "electron-builder"
  },
  "keywords": [
    "nodejs",
    "express",
    "interface"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "nodemon": "^3.0.1",
    "wait-on": "^7.2.0"
  }
}